
<h1><%= @category.name %></h1>

<p>

  <%= @category.name %>

    <table class="article-list"> 
      <% @articles.reverse_each do |article| %>
        <tr data-article_id="<%= article.id %>">
        <td><%= article.title %></td>
        <td><%= article.text %></td>
      </tr>
      <% end %>
 </table>
</p>
